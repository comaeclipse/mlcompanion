{
  "images": {
    "sizes": [120, 180, 240, 360, 400, 600, 800, 1200],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      },
      {
        "protocol": "http",
        "hostname": "**"
      }
    ]
  },
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
